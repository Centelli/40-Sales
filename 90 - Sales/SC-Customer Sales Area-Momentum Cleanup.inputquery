SELECT [Level(Customer Sales Area.Customer Sales Area).CustArea Key], [Level(Customer Sales Area.Customer Sales Area).Inside Sales Key], [Level(Customer Sales Area.Customer Sales Area).Outside Sales Key], [Level(Customer Sales Area.Customer Sales Area).Customer Key], [Level(Customer Sales Area.Customer Sales Area).Source], [Level(Customer Sales Area.Customer Sales Area).Comp Code Key], [Level(Customer Sales Area.Customer Sales Area).Sales Office Key], [Level(Customer Sales Area.Customer Sales Area).Sales Office], [Level(Customer Sales Area.Customer Sales Area).Sales SubRegion], [Level(Customer Sales Area.Customer Sales Area).Sales Region], [Level(Customer Sales Area.Customer Sales Area).RC Currency Key], [Level(Customer Sales Area.Customer Sales Area).Outside Sales], [Level(Customer Sales Area.Customer Sales Area).Outside Sales AG], [Level(Customer Sales Area.Customer Sales Area).Outside Sales Email Address], [Level(Customer Sales Area.Customer Sales Area).Outside Sales AD ID], [Level(Customer Sales Area.Customer Sales Area).RSM AD ID], [Level(Customer Sales Area.Customer Sales Area).RSM], [Level(Customer Sales Area.Customer Sales Area).MD/GM AD ID], [Level(Customer Sales Area.Customer Sales Area).MD/GM], [Level(Customer Sales Area.Customer Sales Area).RMD AD ID], [Level(Customer Sales Area.Customer Sales Area).RMD], [Level(Customer Sales Area.Customer Sales Area).Global Sales AD ID], [Level(Customer Sales Area.Customer Sales Area).Global Sales], [Level(Customer Sales Area.Customer Sales Area).Inside Sales], [Level(Customer Sales Area.Customer Sales Area).Inside Sales AG], [Level(Customer Sales Area.Customer Sales Area).Referred to Key], [Level(Customer Sales Area.Customer Sales Area).Referred to] 
FROM [Level(Customer Sales Area.Customer Sales Area)]
WHERE ([Level(Customer Sales Area.Customer Sales Area).Sales Region] NOT IN ('Americas', 'EMEA', 'APAC') or [Level(Customer Sales Area.Customer Sales Area).Sales Region] IS NULL) and [Level(Customer Sales Area.Customer Sales Area).Sales Office Key] IS NOT NULL and [Level(Customer Sales Area.Customer Sales Area).Source] = 'New Biz'
LEFT OUTER JOIN
SELECT [tbl_dim_sales_region.sales_region_code], [tbl_dim_sales_region.sales_subregion_code], [tbl_dim_sales_region.sales_office_code], [tbl_dim_sales_region.sales_office_name], [tbl_dim_sales_region.regional_currency_code], [tbl_dim_sales_region.sales_office_region]
  FROM [tbl_dim_sales_region]
    ON [tbl_dim_sales_region.sales_office_code] = [Level(Customer Sales Area.Customer Sales Area).Sales Office Key]