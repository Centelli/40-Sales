  [Division Key]=[SQL_I2_vw_VBAK_VBAP.VBAP_SPART]
  [Order Net]=[vOrderNet]
  [Currency Key]=[SQL_I2_vw_VBAK_VBAP.VBAP_WAERK]
  [Order Qty]=[vOrderQty]
