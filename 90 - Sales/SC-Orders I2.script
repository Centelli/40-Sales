  [Sales Org Key]=[SQL_I2_vw_VBAK_VBAP.VBAK_VKORG]
  [Comp Code Key] = [SQL_I2_vw_VBAK_VBAP.VBAK_BUKRS_VF]
//  [Distr Channel]=[SQL_I2_vw_VBAK_VBAP.VBAK_VTWEG]
  [Division Header]=[SQL_I2_vw_VBAK_VBAP.VBAK_SPART]
