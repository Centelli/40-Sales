  [Sales Order Key]=[SQL_I2_vw_VBAK_VBAP.VBAP_VBELN]+''+[SQL_I2_vw_VBAK_VBAP.VBAP_POSNR]
  [SO Header Create Date]=[SQL_I2_vw_VBAK_VBAP.VBAP_ERDAT]
