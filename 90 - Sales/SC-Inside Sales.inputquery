SELECT [KNVP.KNVP_KUNNR], [KNVP.KNVP_VKORG], [KNVP.KNVP_VTWEG], 
[KNVP.KNVP_SPART], [KNVP.KNVP_PARVW], [KNVP.KNVP_PARZA], [KNVP.KNVP_KUNN2], 
[KNVP.KNVP_DEFPA] from [KNVP] 
WHERE [KNVP.KNVP_PARVW] = 'ZM'
ORDER BY [KNVP.KNVP_KUNNR], [KNVP.KNVP_VKORG], [KNVP.KNVP_VTWEG], 
[KNVP.KNVP_SPART], [KNVP.KNVP_DEFPA] desc, [KNVP.KNVP_PARZA]