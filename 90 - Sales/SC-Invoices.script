of plant company but company in Invoice might be different */
IF [SC-Invoices Warmup.T001K_WAERS] = [SC-Invoices Warmup.Comp Currency Key] THEN
  [vPlantToHC] = 1
ELSEIF [SC-Invoices Warmup.T001K_WAERS] = 'CHF' THEN
  [vPlantToHC] = 1 / [v_rate_home_to_chf]
ELSEIF [SC-Invoices Warmup.T001K_WAERS] = [vSalesCurr] THEN
// Plant has same ex-rate as DC, so we can use that
// HUB-703 - exchange rate should not be divided by 1
  [vPlantToHC] = [vExcRatetoHC]
ELSEIF [SC-Invoices Warmup.T001K_WAERS] = [vRegCurrency] THEN
// Triangulate via CHF
// HUB-724 - incorrect ICOGS for HKD
  print 'Triangulating via reg currency:' + [vRegCurrency] + ' to CHF:'+ [v_rate_reg_to_chf] + ': ' + [SC-Invoices Warmup.Comp Currency Key] + ' to CHF:' +  [v_rate_home_to_chf]
  [vPlantToHC] = [v_rate_reg_to_chf] /  [v_rate_home_to_chf]
ELSE
// New rate, so triangulate via CHF
  IF [PlantExRate.Exchange Rate] IS NOT NULL THEN
    [vPlantToHC] = [PlantExRate.Exchange Rate] / [v_rate_home_to_chf]
  ELSE
    print 'ERROR:Plant exchange rate:Plant WAERS:' + [SC-Invoices Warmup.T001K_WAERS] + ':HC:' + [SC-Invoices Warmup.Comp Currency Key]+ ':vExcRatetoHC:' + [vExcRatetoHC]+ ':[v_rate_home_to_chf]:' + [v_rate_home_to_chf] + ':[v_rate_reg_to_chf]:' + [v_rate_reg_to_chf]
  END IF
END IF

