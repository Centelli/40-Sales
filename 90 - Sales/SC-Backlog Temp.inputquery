SELECT [VBEP.VBAP_VBELN], [VBEP.VBAP_POSNR], [VBEP.VBEP_ETENR], [VBEP.VBAK_ERDAT], [VBEP.VBAK_AUDAT], [VBEP.VBAK_VBTYP], [VBEP.VBAK_AUART], [VBEP.VBAK_NETWR], [VBEP.VBAK_WAERK], [VBEP.VBAK_VKORG], [VBEP.VBAK_VTWEG], [VBEP.VBAK_SPART], [VBEP.VBAK_KNUMV], [VBEP.VBAK_VKBUR], [VBEP.VBAK_KALSM], [VBEP.VBAK_KUNNR], [VBEP.VBAK_AEDAT], [VBEP.VBAK_BUKRS_VF], [VBEP.VBAP_MATNR], [VBEP.VBAP_PSTYV], [VBEP.VBAP_ABGRU], [VBEP.VBAP_MEINS], [VBEP.VBAP_SPART], [VBEP.VBAP_NETWR], [VBEP.VBAP_WAERK], [VBEP.VBAP_KWMENG], [VBEP.VBAP_WERKS], [VBEP.VBAP_ERDAT], [VBEP.VBAP_STCUR], [VBEP.VBAP_AEDAT], [VBEP.VBKD_PRSDT], [VBEP.VBUP_LFSTA], [VBEP.VBUP_LFGSA], [VBEP.VBUP_ABSTA], [VBEP.VBUP_GBSTA], [VBEP.VBAP_BEDAE], [VBEP.VBAP_OBJNR], [VBEP.VBAP_UMVKN], [VBEP.VBAP_UMVKZ], [VBEP.VBEP_ETTYP], [VBEP.VBEP_EDATU], [VBEP.VBEP_EZEIT], [VBEP.VBEP_WMENG], [VBEP.VBEP_BMENG], [VBEP.VBEP_VRKME], [VBEP.VBEP_LMENG], [VBEP.VBEP_MEINS], [VBEP.VBEP_WADAT], [VBEP.VBEP_LFREL], [VBEP.VBEP_ETART], [VBEP.VBEP_BDART], [VBEP.VBPA_KUNNR], [VBEP.VBUK_SPSTG], [VBEP.VBAP_MATKL], [VBEP.VBAP_ZZEXTWGVC], [VBEP.VBAP_ZMENG], [VBEP.VBUK_CMGST], [VBEP.VBAP_FKREL], [VBEP.VBUP_FKSAA], [VBEP.VBKD_KURSK], [VBEP.OrderItem_Count_ETENR], [VBEP.OrderItem_Sum_BMENG], [VBEP.Condensed_VBUP_GBSTA], [VBEP.Condensed_Order Source], [VBEP.Condensed_VBKD_FKDAT], [VBEP.Condensed_VBAP Quantity], [VBEP.Condensed_VBUK_SPSTG], [VBEP.Company_Comp Code Key], [VBEP.Company_Comp Currency Key], [VBEP.region_sales_office_code], [VBEP.region_regional_currency_code], [VBEP.Invoice_Invoice Qty], [VBEP.DC Ex Rate], [VBEP.HC Ex Rate], [VBEP.RC Ex Rate], [VBEP.SUM_ORDER_QTY], [VBEP.SUM_ORDER_DC], [VBEP.SUM_ORDER_GC], [VBEP.SUM_ORDER_HC], [VBEP.SUM_ORDER_RC], [VBEP.Customer_Customer Type] FROM [SC-Backlog Warmup] 'VBEP' 
LEFT OUTER JOIN 
SELECT [SC-TCURF.TCURF_FFACT], [SC-TCURF.TCURF_TFACT]
FROM [SC-TCURF]
ON [SC-TCURF.TCURF_KURST] = 'M' AND [SC-TCURF.TCURF_FCURR] = [VBEP.VBAP_WAERK] AND [SC-TCURF.TCURF_TCURR] = [VBEP.Company_Comp Currency Key] AND [SC-TCURF.From Date] <= [VBEP.VBKD_PRSDT] AND [SC-TCURF.To Date] >= [VBEP.VBKD_PRSDT]
LEFT OUTER JOIN
SELECT [BD.VBKD_VBELN], [BD.VBKD_POSNR], [BD.VBKD_PRSDT], [BD.VBKD_KURSK]
  FROM [VBKD] 'BD'
    ON [BD.VBKD_VBELN] = [VBEP.VBAP_VBELN] AND [BD.VBKD_POSNR] = [VBEP.VBAP_POSNR] 
ORDER BY [VBEP.VBAP_VBELN], [VBEP.VBAP_POSNR], [VBEP.VBEP_ETENR] ASCENDING