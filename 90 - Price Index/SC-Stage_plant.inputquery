SELECT [T001W.WERKS], [T001W.NAME1 1], [T001W.Valuation BWKEY], [T001W.KUNNR], [T001W.LIFNR], [T001W.FABKL], [T001W.NAME2], [T001W.STRAS], [T001W.PSTLZ], [T001W.ORT01], [T001W.EKORG], [T001W.VKORG], [T001W.CHAZV], [T001W.BEDPL], [T001W.LAND1], [T001W.REGIO], [T001W.ADRNR], [T001W.IWERK], [T001W.VTWEG], [T001W.SPART], [T001W.SPRAS], [T001W.AWSLS], [T001W.LET01], [T001W.LET02], [T001W.LET03]
  FROM [T001W]
INNER JOIN
SELECT DISTINCT [SC-PriceIndex_Fact2.plant_code]
FROM [SC-PriceIndex_Fact2]
ON [T001W.WERKS]=[SC-PriceIndex_Fact2.plant_code]
  LEFT OUTER JOIN
SELECT [Country.country_name]
  FROM [Country]
  ON [T001W.LAND1]=[Country.country_code]